all:
	MAKE.EXE clean
	TASM.EXE /ml VGA.ASM
	TASM.EXE /ml TIMER.ASM
	TCC.EXE -c -oVGACPP.OBJ VGA.CPP
	TCC.EXE -c -oAPP.OBJ APP.CPP
	TCC.EXE -c -oRENDERER.OBJ RENDERER.CPP
	TCC.EXE -c -oCUBE.OBJ CUBE.CPP
	TCC.EXE -c -oUI.OBJ UI.CPP
	TCC.EXE -c -oKEYBRD.OBJ KEYBRD.CPP
	TCC.EXE -c -oVEC.OBJ VEC.CPP
	TCC.EXE -c -oMATH.OBJ MATH.CPP
	TCC.EXE -c -oTRINGL.OBJ TRINGL.CPP
	TCC.EXE CUBE13H.CPP APP.OBJ VGA.OBJ VGACPP.OBJ RENDERER.OBJ CUBE.OBJ UI.OBJ TIMER.OBJ KEYBRD.OBJ VEC.OBJ MATH.OBJ TRINGL.OBJ
clean:
	DEL CUBE13H.EXE
	DEL CUBE13H.OBJ
	DEL VGA.OBJ
	DEL VGACPP.OBJ
	DEL APP.OBJ
	DEL RENDERER.OBJ
	DEL CUBE.OBJ
	DEL UI.OBJ
	DEL TIMER.OBJ
	DEL KEYBRD.OBJ
	DEL VEC.OBJ
	DEL MATH.OBJ
	DEL TRINGL.OBJ
	