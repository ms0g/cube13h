all:
	MAKE.EXE clean
	TCC.EXE -c -oAPP.OBJ APP.CPP
	TCC.EXE -c -oRENDERER.OBJ RENDERER.CPP
	TCC.EXE -c -oVGA.OBJ VGA.CPP
	TCC.EXE -c -oCUBE.OBJ CUBE.CPP
	TCC.EXE -c -oUI.OBJ UI.CPP
	TCC.EXE -c -oTIMER.OBJ TIMER.CPP
	TCC.EXE CUBE13H.CPP APP.OBJ RENDERER.OBJ VGA.OBJ CUBE.OBJ UI.OBJ TIMER.OBJ
clean:
	DEL CUBE13H.EXE
	DEL CUBE13H.OBJ
	DEL APP.OBJ
	DEL RENDERER.OBJ
	DEL VGA.OBJ
	DEL CUBE.OBJ
	DEL UI.OBJ
	DEL TIMER.OBJ
	