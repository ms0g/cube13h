all:
	MAKE.EXE clean
	TASM.EXE /ml TIMER.ASM
	TCC.EXE -O2 -c -oVGA.OBJ VGA.C
	TCC.EXE -O2 -c -oRENDERER.OBJ RENDERER.C
	TCC.EXE -O2 -c -oKEYBRD.OBJ KEYBRD.C
	TCC.EXE -O2 -c -oVEC.OBJ VEC.C
	TCC.EXE -O2 -c -oMAT.OBJ MAT.C
	TCC.EXE -O2 -c -oMATH.OBJ MATH.C
	TCC.EXE -O2 -c -oTRINGL.OBJ TRINGL.C
	TCC.EXE -O2 -c -oUI.OBJ UI.C
	TCC.EXE -O2 CUBE13H.C VGA.OBJ RENDERER.OBJ TIMER.OBJ KEYBRD.OBJ VEC.OBJ MAT.OBJ MATH.OBJ TRINGL.OBJ UI.OBJ
clean:
	DEL CUBE13H.EXE
	DEL CUBE13H.OBJ
	DEL VGA.OBJ
	DEL RENDERER.OBJ
	DEL TIMER.OBJ
	DEL KEYBRD.OBJ
	DEL VEC.OBJ
	DEL MAT.OBJ
	DEL MATH.OBJ
	DEL TRINGL.OBJ
	DEL UI.OBJ
	